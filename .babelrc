{
  "presets": [
    "babel-preset-expo",
    "react-native",
    "react-native-dotenv",
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "alias": {
              "react-apollo": "./node_modules/react-apollo/react-apollo.browser.umd.js"
            }
          }
        ]
      ]
    }
  }
}
